{
  "name": "rest-api",
  "version": "1.0.0",
  "description": "Rest API for eCOO environment.",
  "scripts": {
    "start:dev": "tsx watch src/infra/server.ts",
    "test": "vitest run --dir src/domain/",
    "test:watch": "vitest --dir src/domain/",
    "test:coverage": "vitest run --coverage",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/infra/http",
    "test:e2e:watch": "vitest --dir src/infra/http",
    "qdrant:seed": "ts-node ./qdrant/seed.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/ejs": "^3.1.3",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.6.5",
    "@types/nodemailer": "^6.4.11",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.0"
  },
  "dependencies": {
    "@fastify/awilix": "^3.2.0",
    "@prisma/client": "^5.4.2",
    "@qdrant/js-client-rest": "^1.6.0",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs-node": "^4.13.0",
    "awilix": "^8.0.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "fastify": "^4.23.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.5",
    "zod": "^3.22.2"
  }
}
